<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffffff"
    android:orientation="vertical" >

    <AbsoluteLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#ffffffff"
        android:orientation="horizontal" >
        
        <ImageView
		    android:id="@+id/map2"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="0dp"
		    android:layout_y="0dp"
		    android:src="@drawable/map2"/>
		
		<ImageView
		    android:id="@+id/text_mypace"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="24dp"
		    android:layout_y="247dp"
		    android:src="@drawable/text_mypace" />

		<ImageView
		    android:id="@+id/condition_number"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="45dp"
		    android:layout_y="388dp"
		    android:src="@drawable/condition_number" />

		<ImageView
		    android:id="@+id/condition_title"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="35dp"
		    android:layout_y="332dp"
		    android:src="@drawable/condition_title" />

		<ImageView
		    android:id="@+id/feedback2"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="26dp"
		    android:layout_y="277dp"
		    android:src="@drawable/feedback2" />
		<ImageView
		    android:id="@+id/group2"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="20dp"
		    android:layout_y="520dp"
		    android:src="@drawable/group" />
		
    </AbsoluteLayout>

</LinearLayout>