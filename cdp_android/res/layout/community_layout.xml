            android:src="@drawable/band_sudo" />

        <ImageView
            android:id="@+id/band6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="250dp"
            android:layout_y="210dp"
            android:src="@drawable/band_gwang" />

        <ImageView
            android:id="@+id/band5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
