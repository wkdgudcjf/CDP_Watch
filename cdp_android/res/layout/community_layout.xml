            android:src="@drawable/band_cycling" />

        <ImageView
            android:id="@+id/band7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="20dp"
            android:layout_y="330dp"
            android:src="@drawable/band_add" />
		
