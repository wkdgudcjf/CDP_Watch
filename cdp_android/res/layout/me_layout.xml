<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffffff"
    android:orientation="vertical" >

    <AbsoluteLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="#ffffffff"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/profile_part"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/profile_part" >
        </ImageView>

        <ImageView
            android:id="@+id/abal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="6dp"
            android:layout_y="67dp"
            android:src="@drawable/abal" />

		<ImageView
		    android:id="@+id/nextpage_icon4"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="190dp"
		    android:layout_y="510dp"
		    android:src="@drawable/nextpage_icon2" />

		<ImageView
		    android:id="@+id/nextpage_icon5"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="200dp"
		    android:layout_y="510dp"
		    android:src="@drawable/nextpage_icon2" />

		<ImageView
		    android:id="@+id/nextpage_icon6"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="210dp"
		    android:layout_y="510dp"
		    android:src="@drawable/nextpage_icon2" />

		<android.support.v4.view.ViewPager
		    android:id="@+id/pictures"
		    android:layout_width="319dp"
		    android:layout_height="208dp"
		    android:layout_x="22dp"
		    android:layout_y="290dp" />

		<ImageView
		    android:id="@+id/text_address"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="26dp"
		    android:layout_y="260dp"
		    android:src="@drawable/text_address" />

		<ImageView
		    android:id="@+id/text_today"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="300dp"
		    android:layout_y="260dp"
		    android:src="@drawable/text_today" />

		<ImageView
		    android:id="@+id/nextpage_icon"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="160dp"
		    android:layout_y="510dp"
		    android:src="@drawable/nextpage_icon" />

		<ImageView
		    android:id="@+id/nextpage_icon2"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="170dp"
		    android:layout_y="510dp"
		    android:src="@drawable/nextpage_icon2" />

		<ImageView
		    android:id="@+id/nextpage_icon3"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="180dp"
		    android:layout_y="510dp"
		    android:src="@drawable/nextpage_icon2" />
		
		<ImageView
		    android:id="@+id/menu_bar_line"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="0dp"
		    android:layout_y="535dp"
		    android:src="@drawable/menu_bar_line"/>

		<android.support.v4.view.ViewPager
		    android:id="@+id/text_timeline"
		    android:layout_width="wrap_content"
		    android:layout_height="112dp"
		    android:layout_x="0dp"
		    android:layout_y="93dp" >
		</android.support.v4.view.ViewPager>
        
    </AbsoluteLayout>

</LinearLayout>